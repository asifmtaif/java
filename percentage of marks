import java.util.Scanner;

public class problem1 {
    public static void main (String[] args) {
        System.out.println("Percentage of CBSE board exam: ");
        Scanner sc = new Scanner (System.in);
        System.out.println(" Marks for sub 1");
        float a= sc.nextFloat();
        System.out.println(" Marks for sub 2");
        float b= sc.nextFloat();
        System.out.println(" Marks for sub 3");
        float c= sc.nextFloat();
        System.out.println(" Marks for sub 4");
        float d= sc.nextFloat();
        System.out.println(" Marks for sub 5");
        float e= sc.nextFloat();


        float total = 500.0F;
        float sum = a+b+c+d+e;
        float percentage= (sum/total) *100;

        System.out.println("The percentage of marks is : ");
        System.out.println(percentage);

    }
}
